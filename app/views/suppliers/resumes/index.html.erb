<%= render 'suppliers/shared/search', url: suppliers_resumes_path, placeholder: '输入简历标签' %>
<%= render 'list', resumes: @resumes %>
<div class='text-center'>
  <%= will_paginate @resumes, renderer: BootstrapPagination::Rails, previous_label: '上一页', next_label: '下一页' %>
</div>
<%= link_to '上传简历', new_suppliers_resume_path, class: 'btn btn-primary btn-lg active' %>
