  <body>
    <%= render partial: "/shared/navbar" %>

 <div class="container">
      <div class="blog-header">
      <div class="row">
        <div class="col-sm-8 blog-main">
          <div class="blog-post">
            <%= yield :job_info %>
        </div><!-- /.blog-main -->

        <div class="col-sm-3 col-sm-offset-1 blog-sidebar">
          <div class="sidebar-module">
            <%= yield :other_job_info %>
          </div>
        </div><!-- /.blog-sidebar -->

      </div><!-- /.row -->

    </div><!-- /.container -->

  </body>
</html>
