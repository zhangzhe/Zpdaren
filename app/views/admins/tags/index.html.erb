<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <td class="default">

          <div id="droppable_nomal_tags">
            <h4>不显示在表头</h4>
            <%= render "nomal_tags" %>
          </div>
        </td>
        <td class="success">

          <div id="droppable_priority_tags" >
            <h4>显示在表头</h4>
            <%= render "priority_tags" %>
          </div>
        </td>
      </tr>
    </thead>
  </table>
</div>

<%= content_for :styles do %>
  <%= stylesheet_link_tag 'drag_and_drop' %>
<% end %>

<%= content_for :scripts do %>
  <%= javascript_include_tag 'drag_and_drop', 'data-turbolinks-track' => true %>
<% end %>
