<%= render 'admins/shared/search', title: '职位列表', url: admins_jobs_path, placeholder: '输入职位名称' %>

<ul class="nav nav-tabs">
  <% active = (params[:state] == "in_hiring") ? "active" : "" %>
  <li role="presentation" class="<%= active %>">
    <%= link_to '进行中的招聘', admins_jobs_path(:state => "in_hiring") %>
  </li>
  <% active = (params[:state] == "un_hiring") ? "active" : "" %>
  <li role="presentation" class="<%= active %>">
    <%= link_to '已完成的招聘', admins_jobs_path(:state => "un_hiring") %>
  </li>
</ul>
<br>
<%= render 'list', jobs: @jobs %>
<%= render 'shared/paginate', objs: @jobs %>
