<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <%= bootstrap_flash %>

      <table class="table">
        <thead>
          <tr>
            <th>Email</th>
            <th>Sign in count</th>
            <th>Last sign in at</th>
            <th>Last sign in ip</th>
            <th colspan="4"></th>
          </tr>
        </thead>

        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= user.email %></td>
            <td><%= user.sign_in_count %></td>
            <td><%= user.last_sign_in_at %></td>
            <td><%= user.last_sign_in_ip %></td>
          </tr>
          <% end %>
        </tbody>
      </table>

    </div>
  </div><!--/row-->

  <footer>
    <p>&copy; Company 2015</p>
  </footer>

</div> <!-- /container -->



