<div class='text-center'>
  <%= will_paginate objs, renderer: BootstrapPagination::Rails, previous_label: '上一页', next_label: '下一页' %>
  <span style="position: absolute;display: inline-block;margin: 26px 10px;color:#737373;"><%= "共#{objs.count}个" if objs.count.to_f / Settings.pagination.page_size.to_f > 1 %></span>
</div>
